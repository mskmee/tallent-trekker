{
  "name": "talent-trekker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start:dev": "vite",
    "build": "tsc && vite build",
    "lint:editor": "editorconfig-checker --exclude node_modules",
    "lint:fs": "ls-lint",
    "lint:css": "npx stylelint \"src/**/*.scss\"",
    "lint:js": "npx eslint \"src/**/*.{ts,tsx}\"",
    "lint:type": "npx tsc --noEmit",
    "lint:css:fix": "npm run lint:css  --fix",
    "lint:prettify": "prettier --check \"**/*.{ts,tsx,json,md,scss,html}\"",
    "lint": "npm run lint:editor && npm run lint:fs && npm run lint:prettify && npm run lint:js && npm run lint:css",
    "prettify": "prettier --write \"**/*.{ts,tsx,json,md,scss,html}\"",
    "preview": "vite preview"
  },
  "dependencies": {
    "@hookform/resolvers": "3.3.2",
    "@reduxjs/toolkit": "^1.9.7",
    "classnames": "^2.3.2",
    "i18next": "^23.6.0",
    "i18next-http-backend": "^2.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.47.0",
    "react-i18next": "^13.3.1",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.17.0",
    "react-toastify": "9.1.3",
    "socket.io-client": "4.7.2",
    "vite-plugin-svgr": "4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@ls-lint/ls-lint": "1.11.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-v8": "^0.34.6",
    "danger": "11.2.8",
    "editorconfig-checker": "5.1.1",
    "eslint": "8.47.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sonarjs": "0.20.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-unicorn": "48.0.1",
    "lint-staged": "13.2.3",
    "prettier": "3.0.1",
    "sass": "^1.69.3",
    "simple-git-hooks": "2.9.0",
    "storybook": "^7.4.6",
    "stylelint": "15.10.2",
    "stylelint-config-standard": "34.0.0",
    "stylelint-config-standard-scss": "11.0.0",
    "stylelint-order": "6.0.3",
    "stylelint-prettier": "4.0.2",
    "ts-node": "^10.9.1",
    "typescript": "5.1.6",
    "vite": "^4.4.5",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "commit-msg": "npx commitlint --edit $1"
  }
}
